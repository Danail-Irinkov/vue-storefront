{
  "elasticsearch": {
    "httpAuth": "",
    "host": "http://vue-storefront-api:8080/api/catalog",
    "index": "vue_storefront_catalog",
    "min_score": 0.02,
    "csrTimeout": 5000,
    "ssrTimeout": 1000,
    "queryMethod": "GET",
    "disableLocalStorageQueriesCache": true,
    "searchScoring": {
      "attributes": {
        "attribute_code": {
          "scoreValues": {
            "attribute_value": {
              "weight": 1
            }
          }
        }
      },
      "fuzziness": 2,
      "cutoff_frequency": 0.01,
      "max_expansions": 3,
      "minimum_should_match": "75%",
      "prefix_length": 2,
      "boost_mode": "multiply",
      "score_mode": "multiply",
      "max_boost": 100,
      "function_min_score": 1
    },
    "searchableAttributes": {
      "name": {
        "boost": 4
      },
      "sku": {
        "boost": 2
      },
      "category.name": {
        "boost": 1
      }
    }
  },
  "storeViews": {
    "multistore": true,
    "commonCache": false,
    "mapStoreUrlsFor": [
      "aa",
      "cctest"
    ],
    "aa": {
      "storeCode": "aa",
      "storeName": "Mashup Store",
      "disabled": false,
      "storeId": 68,
      "name": "Mashup Store",
      "url": "/aa",
      "appendStoreCode": true,
      "elasticsearch": {
        "host": "http://vue-storefront-api:8080/api/catalog",
        "index": "vue_storefront_catalog_aa"
      },
      "tax": {
        "defaultCountry": "BG",
        "defaultRegion": "",
        "calculateServerSide": true,
        "sourcePriceIncludesTax": false
      },
      "i18n": {
        "fullCountryName": "Bulgaria",
        "fullLanguageName": "Bulgarian",
        "defaultCountry": "BG",
        "defaultLanguage": "EN",
        "defaultLocale": "en-US",
        "currencyCode": "EUR",
        "currencySign": "EUR",
        "dateFormat": "HH:mm D-M-YYYY"
      }
    },
    "cctest": {
      "storeCode": "cctest",
      "storeName": "Test Judy Store",
      "disabled": false,
      "storeId": 69,
      "name": "Test Judy Store",
      "url": "/cctest",
      "appendStoreCode": true,
      "elasticsearch": {
        "host": "http://vue-storefront-api:8080/api/catalog",
        "index": "vue_storefront_catalog_cctest"
      },
      "tax": {
        "defaultCountry": "BG",
        "defaultRegion": "",
        "calculateServerSide": true,
        "sourcePriceIncludesTax": false
      },
      "i18n": {
        "fullCountryName": "Bulgaria",
        "fullLanguageName": "Bulgarian",
        "defaultCountry": "BG",
        "defaultLanguage": "EN",
        "defaultLocale": "en-US",
        "currencyCode": "EUR",
        "currencySign": "EUR",
        "dateFormat": "HH:mm D-M-YYYY"
      }
    }
  }
}
